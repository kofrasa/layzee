{
  "name": "layzee",
  "version": "1.0.0",
  "description": "A minimal lazy iterator implementation",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc && touch docs/.nojekyll",
