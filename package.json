    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kofrasa/layzee.git"
  },
  "keywords": [
    "es6",
    "lazy",
    "functional",
    "filter",
    "generators",
    "map",
    "reduce",
    "iterate",
    "iterator",
    "sequence"
  ],
  "author": {
    "name": "Francis Asante",
    "email": "kofrasa@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kofrasa/layzee/issues"
  },
  "homepage": "https://github.com/kofrasa/layzee#readme",
  "files": [
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "esm": "3.2.25",
    "codecov": "3.8.3",
    "nyc": "15.1.0",
    "perf_hooks": "0.0.1",
    "tape": "4.16.2",
    "typescript": "3.8.3",
    "typedoc": "0.23.24"
  }
